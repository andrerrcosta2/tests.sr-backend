{
  "name": "tests.sr-backend",
  "version": "1.0.0",
  "description": "## Objetivo",
  "main": "index.ts",
  "scripts": {
    "build": "tsc",
    "start:dev": "env-cmd -f .env.development nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "start:staging": "env-cmd -f .env.staging ts-node src/index.ts",
    "start": "env-cmd -f .env.production ts-node src/index.ts",
    "test": "env-cmd -f .env.test jest",
    "test:watch": "env-cmd -f .env.test jest --watch",
    "test:debug": "node --inspect ./node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd -f .env.test jest test/e2e",
    "test:e2e:auth": "env-cmd -f .env.test jest test/e2e/auth.controller.test.ts",
    "test:e2e:wallet": "env-cmd -f .env.test jest test/e2e/wallet.controller.test.ts",
    "test:e2e:user": "env-cmd -f .env.test jest test/e2e/user.controller.test.ts",
    "test:e2e:prod-env": "env-cmd -f .env.production jest test/e2e"
  },
